<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawerLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:filterTouchesWhenObscured="true">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/Hex_Gray90"
        android:orientation="vertical">

        <include layout="@layout/toolbar" />

        <ImageView

            android:id="@+id/logo"
            android:layout_width="match_parent"
            android:layout_height="93dp"
            android:src="@drawable/letterhead"
            app:srcCompat="@drawable/letterhead"
            tools:ignore="ContentDescription" />

        <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/Hex_Gray90"
            android:orientation="vertical"
            tools:context=".InOfficeActivity">

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/Hex_Gray90"
            android:orientation="vertical"
            tools:context=".InOfficeActivity">



            <TextView
                android:id="@+id/officeForm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_centerHorizontal="true"
                android:layout_marginTop="12dp"
                android:fontFamily="@font/calibri"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="@string/inOffice_head"
                android:textColor="@color/Color_Panto"
                android:textSize="25sp"
                android:textStyle="bold" />

            <CheckBox
                android:id="@+id/CB1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_below="@+id/officeForm"
                android:layout_marginTop="35dp"
                android:fontFamily="@font/calibri"
                android:text="@string/spa"
                android:textColor="@color/Color_Panto"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editText1"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/officeForm"
                android:layout_marginStart="150dp"
                android:layout_marginTop="32dp"
                android:ems="10"
                android:fontFamily="@font/calibri"
                android:inputType="time"
                android:labelFor="@id/editText1"
                android:textColor="@color/Color_Panto"
                android:textSize="12sp"
                tools:ignore="Autofill" />

            <CheckBox
                android:id="@+id/CB2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_below="@+id/CB1"
                android:layout_marginTop="18dp"
                android:fontFamily="@font/calibri"
                android:text="@string/dayProgram"
                android:textColor="@color/Color_Panto"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editText2"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/editText1"
                android:layout_marginStart="150dp"
                android:layout_marginTop="14dp"
                android:ems="10"
                android:fontFamily="@font/calibri"
                android:inputType="time"
                android:labelFor="@id/editText2"
                android:textColor="@color/Color_Panto"
                android:textSize="12sp"
                tools:ignore="Autofill" />

            <CheckBox
                android:id="@+id/CB3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_below="@+id/CB2"
                android:layout_marginTop="18dp"
                android:fontFamily="@font/calibri"
                android:text="@string/ct"
                android:textColor="@color/Color_Panto"
                android:textSize="12sp" />

            <EditText
                android:id="@+id/editText3"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/editText2"
                android:layout_marginStart="150dp"
                android:layout_marginTop="14dp"
                android:ems="10"
                android:fontFamily="@font/calibri"
                android:inputType="time"
                android:labelFor="@id/editText3"
                android:textColor="@color/Color_Panto"
                android:textSize="12sp"
                tools:ignore="Autofill" />

            <CheckBox
                android:id="@+id/CB4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_below="@+id/CB3"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/calibri"
                android:text="@string/training1"
                android:textColor="@color/Color_Panto"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editText4"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/editText3"
                android:layout_marginStart="150dp"
                android:layout_marginTop="16dp"
                android:ems="10"
                android:fontFamily="@font/calibri"
                android:inputType="time"
                android:labelFor="@id/editText4"
                android:textColor="@color/Color_Panto"
                android:textSize="12sp"
                tools:ignore="Autofill" />

            <CheckBox
                android:id="@+id/CB5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_below="@+id/CB4"
                android:layout_marginTop="18dp"
                android:fontFamily="@font/calibri"
                android:text="@string/outreach1"
                android:textColor="@color/Color_Panto"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editText5"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/editText4"
                android:layout_marginStart="150dp"
                android:layout_marginTop="14dp"
                android:ems="10"
                android:fontFamily="@font/calibri"
                android:inputType="time"
                android:labelFor="@id/editText5"
                android:textColor="@color/Color_Panto"
                android:textSize="12sp"
                tools:ignore="Autofill" />

            <CheckBox
                android:id="@+id/CB6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_below="@+id/CB5"
                android:layout_marginTop="18dp"
                android:fontFamily="@font/calibri"
                android:text="@string/art_therapy"
                android:textColor="@color/Color_Panto"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editText6"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/editText5"
                android:layout_marginStart="150dp"
                android:layout_marginTop="17dp"
                android:ems="10"
                android:fontFamily="@font/calibri"
                android:inputType="time"
                android:labelFor="@id/editText6"
                android:textColor="@color/Color_Panto"
                android:textSize="12sp"
                tools:ignore="Autofill" />

            <CheckBox
                android:id="@+id/CB7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_below="@+id/CB6"
                android:layout_marginTop="18dp"
                android:fontFamily="@font/calibri"
                android:text="@string/music_therapy"
                android:textColor="@color/Color_Panto"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editText7"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/editText6"
                android:layout_marginStart="150dp"
                android:layout_marginTop="14dp"
                android:ems="10"
                android:fontFamily="@font/calibri"
                android:inputType="time"
                android:labelFor="@id/editText7"
                android:textColor="@color/Color_Panto"
                android:textSize="12sp"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/textViewPerson"
                android:layout_width="126dp"
                android:layout_height="wrap_content"
                android:layout_columnWeight="1"
                android:layout_below="@+id/CB7"
                android:layout_marginTop="18dp"
                android:fontFamily="@font/calibri"
                android:text="@string/numberOfPerson2"
                android:textColor="@color/Color_Panto"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/editTextNumPerson2"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/editText7"
                android:layout_marginStart="150dp"
                android:layout_marginTop="14dp"
                android:ems="10"
                android:fontFamily="@font/calibri"
                android:inputType="number"
                android:labelFor="@id/editTextNumPerson2"
                android:textColor="@color/Color_Panto"
                android:textSize="15sp"
                tools:ignore="Autofill" />

            <Button
                android:id="@+id/btn_directSubmit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/editTextNumPerson2"
                android:layout_centerInParent="true"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/calibri"
                android:text="@string/submit"
                android:textColor="@color/Color_Panto"
                android:textStyle="bold" />
            </RelativeLayout>
        </ScrollView>

</LinearLayout>
    <include layout="@layout/drawer" />
</android.support.v4.widget.DrawerLayout>